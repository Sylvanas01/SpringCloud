<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mango.demo.dao.IndexDao">
    <insert id="addUser" parameterType="com.mango.demo.entity.User">
        INSERT INTO user
        (`address`,`phone`,`telephone`,`mz`,`xl`,`time`,`delFlag`)
        VALUES
        (#{address},#{phone},#{telephone},#{mz},#{xl},#{time},#{delFlag});
    </insert>
</mapper>
