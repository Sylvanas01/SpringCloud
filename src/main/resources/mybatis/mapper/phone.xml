<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mango.demo.dao.PhoneDao">
    <insert id="addMessagePhone" parameterType="com.mango.demo.entity.MessagePhone">
        INSERT INTO messagephone
        (`orderId`,`typeDetailId`,`suppilerName`,`phones`)
        VALUES
        (#{orderId},#{typeDetailId},#{suppilerName},#{phones});
    </insert>

</mapper>
